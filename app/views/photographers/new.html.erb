<div class="studio-theme-wrapper" style="margin-left:60px;margin-right:60px">
  <div class="col-sm-12 main">
    <br />
    <div class="row">
      <div class="col-lg-12">
        <h1 class="page-header">Create Studio</h1>
      </div>
    </div><!--/.row-->

    <div class="row">
			<div class="col-lg-12">
				<div class="panel panel-default">
					<div class="panel-heading">Information</div>
					<div class="panel-body">
						<div class="col-md-6">
							<form role="form">

								<div class="form-group">
									<label>Brand name</label>
									<input class="form-control" placeholder="">
								</div>

							</div>
							<div class="col-md-6">

								<div class="form-group">
									<label>Available photoshooting places</label>
									<div class="checkbox">
										<label>
											<input type="checkbox" value="">Sai Gon
										</label>
									</div>
									<div class="checkbox">
										<label>
											<input type="checkbox" value="">Da Nang
										</label>
									</div>
									<div class="checkbox">
										<label>
											<input type="checkbox" value="">Nha Trang
										</label>
									</div>
									<div class="checkbox">
										<label>
											<input type="checkbox" value="">Da Lat
										</label>
									</div>
								</div>

								<button type="submit" class="btn btn-primary">Submit</button>
                <%= link_to root_path do %>
                  <button type="reset" class="btn btn-default">Cancel</button>
                <% end %>
							</div>
						</form>
					</div>
				</div>
			</div><!-- /.col-->
		</div><!-- /.row -->

  </div>	<!--/.main-->
</div>
